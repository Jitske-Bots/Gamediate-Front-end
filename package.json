{
  "name": "gamediate-front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "prod": "ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom --code-coverage",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && npm run test && npm run build:prod",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.0",
    "@angular/http": "^7.2.16",
    "@angular/material": "^12.2.11",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@microsoft/signalr": "^6.0.0",
    "@types/signalr": "^2.2.37",
    "babel-jest": "^21.2.0",
    "constants": "^0.0.2",
    "cypress": "^9.2.1",
    "module": "^1.2.5",
    "ngx-cookie-service": "^13.1.2",
    "rxjs": "~6.6.0",
    "stream": "^0.0.2",
    "tsc": "^2.0.3",
    "tslib": "^2.3.0",
    "util": "^0.12.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular/cli": "~12.2.6",
    "@angular/compiler-cli": "~12.2.0",
    "@nrwl/cypress": "^13.4.3",
    "@nrwl/workspace": "^13.4.3",
    "@types/jasmine": "^3.10.3",
    "@types/node": "^12.11.1",
    "assert": "^2.0.0",
    "jasmine": "^4.0.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-sonarqube-unit-reporter": "^0.0.23",
    "puppeteer": "^13.0.1",
    "sonar-scanner": "^3.1.0",
    "typescript": "~4.3.5"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "resolve.fallback": {
    "assert": false
  }
}
